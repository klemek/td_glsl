bool char(vec2 pos, int code)
{
    if (pos.x < 0.0 || pos.y < 0.0 || pos.x >= 1.0 || pos.y >= 1.0) {
        return false;
    }
    ivec2 pos2 = ivec2(pos.x * 8.0, (1 - pos.y) * 8.0);
    ivec2 subpos = pos2 % 4;
    int v = int(pow(2, subpos.y * 4 + subpos.x));
    int d = int(pos2.y * 0.25) * 2 + int(pos2.x * 0.25);
    return (cp437[code * 4 + d] & v) > 0;
}

void main()
{
    // start

	vec2 uv0 = vUV.st;
    float ratio = iResolution.x / iResolution.y;
    vec2 uv1 = (uv0 - .5) * vec2(ratio, 1);

    // controls

    float zoom = magic(iF2, iB2, 447);

    float start_char = magic(iF3, iB3, 965, 4);

    float char_delta = magic(iF4, iB4, 216, 4);

    // logic

    vec2 uv2 = uv1;

    uv2 *= zoom * 20 + 3;

    uv2 += iTime * iTempo / 60;

    uv2 = mod(uv2, 100) + 100;
    
    int code = int(0x100 * start_char + rand(ivec2(uv2)) * 0x100 * char_delta) % 0xFF;

    uv2 = mod(uv2, 1);

    vec3 c = vec3(char(uv2, code) ? 1 : 0);

    // output

    fragColor = TDOutputSwizzle(vec4(c, 1.));
}